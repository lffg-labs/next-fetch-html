{
  "name": "next-fetch-html",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn eslint ."
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "eslint": "7.x",
    "eslint-config-lffg": "^21.0.0-1",
    "eslint-config-next": "12.1.0",
    "prettier": "^2.5.1",
    "typescript": "4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "lffg"
    ],
    "parserOptions": {
      "warnOnUnsupportedTypeScriptVersion": false
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": 0,
      "import/no-default-export": 0
    }
  },
  "prettier": "eslint-config-lffg/prettier"
}
